  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "swc src -d dist --config-file .swcrc && rsync -rmv --include='*/' --include='*.cpp' --exclude='*' src/ dist/ > /dev/null",
    "start": "node --expose-gc --experimental-import-meta-resolve --experimental-specifier-resolution=node --enable-source-maps dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles"
  },
  "author": "tiger0132",
  "type": "module",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.69",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.27",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/klaw": "^3.0.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "jest-extended": "^4.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "klaw": "^4.1.0"
  }
